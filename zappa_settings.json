{
  "prod": {
    "app_function": "redash_emailer.aws_lambda",
    "aws_region": "us-west-1",
    "project_name": "redash-emailer",
    "runtime": "python3.12",
    "cloudwatch_log_level": "INFO",
    "cloudwatch_data_trace": true,
    "keep_warm": false,
    "log_level": "INFO",
    "s3_bucket": "zappa-fmkb1g17t",
    "manage_roles": false, // required to specify role_name below
    "role_name": "test_python",
    "slim_handler": false,
    "apigateway_enabled": false,
    "vpc_config": {
      "SubnetIds": [
        "subnet-d8385681"
      ],
      "SecurityGroupIds": [
        "sg-c2ee0da5"
      ]
    },
    "exclude": [
      ".git",
      ".pytest_cache"
    ],
    "events": [
      {
        "function": "redash_emailer.aws_lambda",
        "name": "wednesday-accounting-report",
        "expression": "cron(35 14 ? * WED *)",
        "kwargs": {
          "event_name": "wednesday-accounting-report",
          "query_id": "12710",
          "to_address": "rsmus-share@moveon.org",
          "from_address": "MoveOn Reports <reports-noreply@moveon.org>",
          "subject": "MoveOn Braintree disbursements",
          "body": "Braintree disbursements in attached CSV."
        }
      },
      {
        "function": "redash_emailer.aws_lambda",
        "name": "friday-accounting-report",
        "expression": "cron(35 14 ? * FRI *)",
        "kwargs": {
          "event_name": "friday-accounting-report",
          "query_id": "12710",
          "to_address": "rsmus-share@moveon.org",
          "from_address": "MoveOn Reports <reports-noreply@moveon.org>",
          "subject": "MoveOn Braintree disbursements",
          "body": "Braintree disbursements in attached CSV."
        }
      },
      {
        "function": "redash_emailer.aws_lambda",
        "name": "monthly-accounting-report",
        "expression": "cron(35 14 2 * ? *)",
        "kwargs": {
          "event_name": "monthly-accounting-report",
          "query_id": "12710",
          "to_address": "rsmus-share@moveon.org",
          "from_address": "MoveOn Reports <reports-noreply@moveon.org>",
          "subject": "MoveOn Braintree disbursements",
          "body": "Braintree disbursements in attached CSV."
        }
      }
    ]
  }
}
